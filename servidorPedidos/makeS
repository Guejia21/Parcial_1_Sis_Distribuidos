
# This is a template Makefile generated by rpcgen

# Parameters

CLIENT = 
SERVER = ejecutableServidor

SOURCES_CLNT.c = 
SOURCES_CLNT.h = 
SOURCES_SVC.c = servidor.c
SOURCES_SVC.h = IntClienteSerPedidos.h IntSerPedidosSerDisplay.h IntCocineroSerPedidos.h
SOURCES.x = IntClienteSerPedidos.x IntCocineroSerPedidos.x

TARGETS_SVC.c = IntClienteSerPedidos_svc.c   IntClienteSerPedidos_xdr.c IntSerPedidosSerDisplay_clnt.c   IntSerPedidosSerDisplay_xdr.c
TARGETS_CLNT.c =     
TARGETS = IntClienteSerPedidos.h IntClienteSerPedidos_xdr.c   IntClienteSerPedidos_svc.c  IntCocineroSerPedidos.h IntCocineroSerPedidos_xdr.c IntCocineroSerPedidos_clnt.c IntCocineroSerPedidos_svc.c IntSerPedidosSerDisplay.h IntSerPedidosSerDisplay_xdr.c IntSerPedidosSerDisplay_clnt.c 

OBJECTS_CLNT = $(SOURCES_CLNT.c:%.c=%.o) $(TARGETS_CLNT.c:%.c=%.o)
OBJECTS_SVC = $(SOURCES_SVC.c:%.c=%.o) $(TARGETS_SVC.c:%.c=%.o)
# Compiler flags 

CFLAGS += -g -I /usr/include/tirpc
LDLIBS += -lnsl -ltirpc
RPCGENFLAGS = 

# Targets 

all : $(CLIENT) $(SERVER)

#Se compila un archivo a la vez
$(TARGETS) : $(SOURCES.x)
	@for src in $(SOURCES.x); do rpcgen $(RPCGENFLAGS) $$src; done

$(OBJECTS_CLNT) : $(SOURCES_CLNT.c) $(SOURCES_CLNT.h) $(TARGETS_CLNT.c) 

$(OBJECTS_SVC) : $(SOURCES_SVC.c) $(SOURCES_SVC.h) $(TARGETS_SVC.c) 

$(CLIENT) : $(OBJECTS_CLNT) 
	$(LINK.c) -o $(CLIENT) $(OBJECTS_CLNT) $(LDLIBS) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.c) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS)

 clean:
	 $(RM) core $(TARGETS) $(OBJECTS_CLNT) $(OBJECTS_SVC) $(CLIENT) $(SERVER)

